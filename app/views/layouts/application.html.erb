<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= content_for?(:title) ? yield(:title) : "UCN Parking" %></title>
  <meta name="description" content="">
  <meta name="author" content="">
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= yield(:head) %>
</head>
<body>
  <div id="main" role="main">
    <div class="container">
      <div class="content">

        <%= yield %>

        <footer class="footer">
          <div class="row">
            Using memcache: <%= params[:skip_memcache] ? 'No' : 'Yes' %>
          </div>
        </footer>

      </div>
    </div> <!--! end of .container -->
  </div> <!--! end of #main -->
</body>
</html>